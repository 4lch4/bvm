{
  "name": "@4lch4/bvm",
  "displayName": "bvm",
  "version": "0.0.0",
  "description": "A small CLI tool to manage multiple versions of Bun.",
  "keywords": [
    "4lch4"
  ],
  "homepage": "https://github.com/4lch4/bvm",
  "bugs": {
    "url": "https://github.com/4lch4/bvm/issues",
    "email": "bugs@4lch4.email"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/4lch4/bvm.git"
  },
  "license": "MIT",
  "author": {
    "name": "4lch4",
    "email": "hey@4lch4.email",
    "url": "https://4lch4.com"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "bin": {
    "bvm": "./dist/index.js"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --target bun --outfile ./dist/index.js",
    "dev": "bun run --watch src/index.ts",
    "lint": "prettier -c .",
    "prepublishOnly": "bun run build",
    "pretty": "prettier --write .",
    "start": "bun run dist/index.js",
    "test": "bun test"
  },
  "dependencies": {
    "@4lch4/backpack": "^1.3.6",
    "@octokit/core": "^5.0.2",
    "@octokit/plugin-paginate-rest": "^9.1.5",
    "@octokit/rest": "^20.0.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.4",
    "bun-types": "^1.0.15",
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.7",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
